                <Image component="BottomLeftCorner" imageset="Resummoned" image="Rounded_Square_Small_BL" />
                <Image component="TopEdge" imageset="Resummoned" image="Rounded_Square_Small_T" />
                <Image component="Background" imageset="Resummoned" image="Rounded_Square_Small_M" />
                <Image component="BottomEdge" imageset="Resummoned" image="Rounded_Square_Small_B" />
                <Image component="TopRightCorner" imageset="Resummoned" image="Rounded_Square_Small_TR" />
