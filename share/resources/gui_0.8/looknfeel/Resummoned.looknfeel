                <VertFormat type="Stretched" />
                <HorzFormat type="Stretched" />
            </ImageryComponent>
        </ImagerySection>
        
        <ImagerySection name="label">
            <TextComponent>
                <Area>
                    <Dim type="LeftEdge" >
                        <ImageDim imageset="Resummoned" image="HeaderBarSortUp" dimension="Width">
                            <OperatorDim op="Add">
                                <AbsoluteDim value="5" />
