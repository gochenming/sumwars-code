                <Dim type="Width" ><UnifiedDim scale="0.5" type="Height" /></Dim>
                <Dim type="Height" ><UnifiedDim scale="0.5" type="Height" /></Dim>
            </Area>
            <HorzAlignment type="RightAligned" />
            <Property name="GlyphImage" value="Resummoned/GlyphArrowDown" />
        </Child>
        <StateImagery name="Enabled" />
        <StateImagery name="Disabled" />
    </WidgetLook>



    <!--
    ***************************************************
        Resummoned/MultiLineEditbox
        (based on the Taharez Look)
    ***************************************************
    -->
    <WidgetLook name="Resummoned/MultiLineEditbox">
        <PropertyDefinition name="NormalTextColour" initialValue="FFFFFFFF" redrawOnWrite="true" />
        <PropertyDefinition name="SelectedTextColour" initialValue="FFFFDDDD" redrawOnWrite="true" />
        <PropertyDefinition name="ActiveSelectionColour" initialValue="FFFF8080" redrawOnWrite="true" />
        <PropertyDefinition name="InactiveSelectionColour" initialValue="FFB15050" redrawOnWrite="true" />

        <!-- The size of the scrollbar + buttons -->
        <PropertyDefinition name="ScrollBarPreferredSize" initialValue="24" redrawOnWrite="true" layoutOnWrite="false" />

        <Property name="MouseCursorImage" value="ResummonedCursors/MouseTextBar" />
        <Property name="SelectionBrushImage" value="Resummoned/MultiLineEditboxSelectionBrush" />
        <NamedArea name="TextArea">
            <Area>
                <Dim type="LeftEdge" ><ImageDim imageset="Resummoned" image="MultiLineEditboxLeft" dimension="Width" /></Dim>
                <Dim type="TopEdge" ><ImageDim imageset="Resummoned" image="MultiLineEditboxTop" dimension="Height" /></Dim>
                <Dim type="RightEdge" >
                    <UnifiedDim scale="1" type="RightEdge">
                        <OperatorDim op="Subtract">
                            <ImageDim imageset="Resummoned" image="MultiLineEditboxRight" dimension="Width" />
                        </OperatorDim>
                    </UnifiedDim>
                </Dim>
                <Dim type="BottomEdge" >
                    <UnifiedDim scale="1" type="BottomEdge">
                        <OperatorDim op="Subtract">
                            <ImageDim imageset="Resummoned" image="MultiLineEditboxBottom" dimension="Height" />
                        </OperatorDim>
                    </UnifiedDim>
                </Dim>
            </Area>
        </NamedArea>
        <NamedArea name="TextAreaHScroll">
            <Area>
                <Dim type="LeftEdge" ><ImageDim imageset="Resummoned" image="MultiLineEditboxLeft" dimension="Width" /></Dim>
                <Dim type="TopEdge" ><ImageDim imageset="Resummoned" image="MultiLineEditboxTop" dimension="Height" /></Dim>
                <Dim type="RightEdge" >
                    <UnifiedDim scale="1" type="RightEdge">
                        <OperatorDim op="Subtract">
                            <ImageDim imageset="Resummoned" image="MultiLineEditboxRight" dimension="Width" />
                        </OperatorDim>
                    </UnifiedDim>
                </Dim>
                <Dim type="BottomEdge" >
                    <UnifiedDim scale="1" type="BottomEdge">
                        <OperatorDim op="Subtract">
                            <WidgetDim widget="__auto_hscrollbar__" dimension="Height" />
                        </OperatorDim>
                    </UnifiedDim>
                </Dim>
            </Area>
        </NamedArea>
        <NamedArea name="TextAreaVScroll">
            <Area>
                <Dim type="LeftEdge" ><ImageDim imageset="Resummoned" image="MultiLineEditboxLeft" dimension="Width" /></Dim>
                <Dim type="TopEdge" ><ImageDim imageset="Resummoned" image="MultiLineEditboxTop" dimension="Height" /></Dim>
                <Dim type="RightEdge" >
                    <UnifiedDim scale="1" type="RightEdge">
                        <OperatorDim op="Subtract">
                            <WidgetDim widget="__auto_vscrollbar__" dimension="Width" />
                        </OperatorDim>
                    </UnifiedDim>
                </Dim>
                <Dim type="BottomEdge" >
                    <UnifiedDim scale="1" type="BottomEdge">
                        <OperatorDim op="Subtract">
                            <ImageDim imageset="Resummoned" image="MultiLineEditboxBottom" dimension="Height" />
                        </OperatorDim>
                    </UnifiedDim>
                </Dim>
            </Area>
        </NamedArea>
        <NamedArea name="TextAreaHVScroll">
            <Area>
                <Dim type="LeftEdge" ><ImageDim imageset="Resummoned" image="MultiLineEditboxLeft" dimension="Width" /></Dim>
                <Dim type="TopEdge" ><ImageDim imageset="Resummoned" image="MultiLineEditboxTop" dimension="Height" /></Dim>
                <Dim type="RightEdge" >
                    <UnifiedDim scale="1" type="RightEdge">
                        <OperatorDim op="Subtract">
                            <WidgetDim widget="__auto_vscrollbar__" dimension="Width" />
                        </OperatorDim>
                    </UnifiedDim>
                </Dim>
                <Dim type="BottomEdge" >
                    <UnifiedDim scale="1" type="BottomEdge">
                        <OperatorDim op="Subtract">
                            <WidgetDim widget="__auto_hscrollbar__" dimension="Height" />
                        </OperatorDim>
                    </UnifiedDim>
                </Dim>
            </Area>
        </NamedArea>
        <Child  type="Resummoned/HorizontalScrollbar" nameSuffix="__auto_hscrollbar__">
            <Area>
                <Dim type="LeftEdge" ><AbsoluteDim value="3" /></Dim>
                <Dim type="TopEdge" ><AbsoluteDim value="-4" /></Dim>
                <Dim type="Width" >
                    <UnifiedDim scale="1" type="Width">
                        <OperatorDim op="Subtract">
                            <PropertyDim name="ScrollBarPreferredSize">
                                <OperatorDim op="Add">
                                    <AbsoluteDim value="5" />
                                </OperatorDim>
                            </PropertyDim>
                        </OperatorDim>
                    </UnifiedDim>
                </Dim>
                <Dim type="Height">
                    <PropertyDim name="ScrollBarPreferredSize" />
                </Dim>
            </Area>
            <VertAlignment type="BottomAligned" />
        </Child>
        <Child  type="Resummoned/VerticalScrollbar" nameSuffix="__auto_vscrollbar__">
            <Area>
                <Dim type="LeftEdge" ><AbsoluteDim value="-3"  /></Dim>
                <Dim type="TopEdge" ><AbsoluteDim value="3" /></Dim>
                <Dim type="Width" >
                    <PropertyDim name="ScrollBarPreferredSize" />
                </Dim>
                <Dim type="Height" >
                    <UnifiedDim scale="1" type="Height">
                        <OperatorDim op="Subtract">
                            <PropertyDim name="ScrollBarPreferredSize">
                                <OperatorDim op="Add">
                                    <AbsoluteDim value="7" />
                                </OperatorDim>
                            </PropertyDim>
                        </OperatorDim>
                    </UnifiedDim>
                </Dim>
            </Area>
            <HorzAlignment type="RightAligned" />
        </Child>
        <ImagerySection name="main">
            <FrameComponent>
                <Area>
                    <Dim type="LeftEdge" ><AbsoluteDim value="0" /></Dim>
                    <Dim type="TopEdge" ><AbsoluteDim value="0" /></Dim>
                    <Dim type="Width" ><UnifiedDim scale="1" type="Width" /></Dim>
                    <Dim type="Height" ><UnifiedDim scale="1" type="Height" /></Dim>
