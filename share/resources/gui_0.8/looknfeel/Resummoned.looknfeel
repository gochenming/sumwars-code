                <Image component="BottomRightCorner" imageset="Resummoned" image="Rounded_Square_Small_BR" />
            </FrameComponent>
        </ImagerySection>
        
        <ImagerySection name="glyph">
            <ImageryComponent>
                <Area>
                    <Dim type="LeftEdge">
                        <ImageDim imageset="Resummoned" image="Rounded_Square_Small_Calc_L" dimension="Width" />
                    </Dim>
                    <Dim type="TopEdge">
                        <ImageDim imageset="Resummoned" image="Rounded_Square_Small_Calc_T" dimension="Height" />
                    </Dim>
                    <Dim type="Width" >
                        <UnifiedDim scale="1" type="Width" >
                            <OperatorDim op="Subtract">
                                <ImageDim imageset="Resummoned" image="Rounded_Square_Small_Calc_L" dimension="Width">
                                    <OperatorDim op="Multiply">
                                        <AbsoluteDim value="2" />
                                    </OperatorDim>
                                </ImageDim>
                            </OperatorDim>
                        </UnifiedDim>
                    </Dim>
                    <Dim type="Height" >
                        <UnifiedDim scale="1" type="Height">
                            <OperatorDim op="Subtract">
                                <ImageDim imageset="Resummoned" image="Rounded_Square_Small_Calc_T" dimension="Height">
                                    <OperatorDim op="Multiply">
                                        <AbsoluteDim value="2" />
                                    </OperatorDim>
                                </ImageDim>
                            </OperatorDim>
                        </UnifiedDim>
                    </Dim>
                </Area>
                <ImageProperty name="Image" />
                <VertFormatProperty name="VertImageFormatting" />
                <HorzFormatProperty name="HorzImageFormatting" />
            </ImageryComponent>
        </ImagerySection>
        
        <ImagerySection name="hover">
            <FrameComponent>
                <Area>
                    <Dim type="LeftEdge">
                        <ImageDim imageset="Resummoned" image="Rounded_Square_Small_Calc_L" dimension="Width" />
                    </Dim>
                    <Dim type="TopEdge">
                        <ImageDim imageset="Resummoned" image="Rounded_Square_Small_Calc_T" dimension="Height" />
                    </Dim>
                    <Dim type="Width" >
                        <UnifiedDim scale="1" type="Width" >
                            <OperatorDim op="Subtract">
                                <ImageDim imageset="Resummoned" image="Rounded_Square_Small_Calc_L" dimension="Width">
                                    <OperatorDim op="Multiply">
                                        <AbsoluteDim value="2" />
                                    </OperatorDim>
                                </ImageDim>
                            </OperatorDim>
                        </UnifiedDim>
                    </Dim>
                    <Dim type="Height" >
                        <UnifiedDim scale="1" type="Height">
                            <OperatorDim op="Subtract">
                                <ImageDim imageset="Resummoned" image="Rounded_Square_Small_Calc_T" dimension="Height">
                                    <OperatorDim op="Multiply">
                                        <AbsoluteDim value="2" />
                                    </OperatorDim>
                                </ImageDim>
                            </OperatorDim>
                        </UnifiedDim>
                    </Dim>
                </Area>
                <Image component="TopLeftCorner" imageset="Resummoned" image="Button_Background_TL" />
                <Image component="LeftEdge" imageset="Resummoned" image="Button_Background_L" />
                <Image component="BottomLeftCorner" imageset="Resummoned" image="Button_Background_BL" />
                <Image component="TopEdge" imageset="Resummoned" image="Button_Background_T" />
                <Image component="Background" imageset="Resummoned" image="Button_Background_M" />
                <Image component="BottomEdge" imageset="Resummoned" image="Button_Background_B" />
                <Image component="TopRightCorner" imageset="Resummoned" image="Button_Background_TR" />
                <Image component="RightEdge" imageset="Resummoned" image="Button_Background_R" />
                <Image component="BottomRightCorner" imageset="Resummoned" image="Button_Background_BR" />

            </FrameComponent>
        </ImagerySection>
        
        <StateImagery name="Normal">
            <Layer>
                <Section section="hover">
                    <ColourProperty name="BackgroundColour" />
                </Section>
                <Section section="border_section">
                    <ColourProperty name="BorderColour" />
                </Section>
