                <Image component="BottomLeftCorner" imageset="Resummoned" image="TitlebarBorder_BL" />
                <Image component="TopEdge" imageset="Resummoned" image="TitlebarBorder_T" />
                <Image component="Background" imageset="Resummoned" image="TitlebarBorder_M" />
                <Image component="BottomEdge" imageset="Resummoned" image="TitlebarBorder_B" />
                <Image component="TopRightCorner" imageset="Resummoned" image="TitlebarBorder_TR" />
                <Image component="RightEdge" imageset="Resummoned" image="TitlebarBorder_R" />
                <Image component="BottomRightCorner" imageset="Resummoned" image="TitlebarBorder_BR" />
            </FrameComponent>
        </ImagerySection>

        <ImagerySection name="background_section">
            <ImageryComponent>
                <Area>
                    <Dim type="LeftEdge" ><AbsoluteDim value="4" /></Dim>
                    <Dim type="TopEdge" ><AbsoluteDim value="4" /></Dim>
                    <Dim type="Width" >
                        <UnifiedDim scale="1" type="Width">
                            <OperatorDim op="Subtract">
                                <AbsoluteDim value="8" />
                            </OperatorDim>
                        </UnifiedDim>
                    </Dim>
                    <Dim type="Height" >
                        <UnifiedDim scale="1" type="Height">
                            <OperatorDim op="Subtract">
                                <AbsoluteDim value="8" />
                            </OperatorDim>
                        </UnifiedDim>
                    </Dim>
                </Area>
                <Image component="Background" imageset="Resummoned" image="TitlebarBackground" />
                <VertFormat type="Stretched" />
                <HorzFormat type="Stretched" />
            </ImageryComponent>
        </ImagerySection>


        <ImagerySection name="background_highlight">
            <ImageryComponent>
                <Area>
                    <Dim type="LeftEdge" ><AbsoluteDim value="4" /></Dim>
                    <Dim type="TopEdge" ><AbsoluteDim value="4" /></Dim>
                    <Dim type="Width" >
                        <UnifiedDim scale="1" type="Width">
                            <OperatorDim op="Subtract">
                                <AbsoluteDim value="8" />
                            </OperatorDim>
                        </UnifiedDim>
                    </Dim>
                    <Dim type="Height" >
                        <UnifiedDim scale="1" type="Height">
                            <OperatorDim op="Subtract">
                                <AbsoluteDim value="8" />
                            </OperatorDim>
                        </UnifiedDim>
                    </Dim>
                </Area>
                <Image component="Background" imageset="Resummoned" image="TitlebarBackghighlight" />
                <VertFormat type="Stretched" />
                <HorzFormat type="Stretched" />
            </ImageryComponent>
        </ImagerySection>
        
        <ImagerySection name="caption">
            <TextComponent>
                <Area>
                    <Dim type="LeftEdge" >
                        <ImageDim imageset="Resummoned" image="TitlebarBorder_L" dimension="Width">
                            <OperatorDim op="Add">
                                <ImageDim imageset="Resummoned" image="EmptyGlyph24" dimension="Width" />
                            </OperatorDim>
                        </ImageDim>
                    </Dim>
                    <Dim type="TopEdge" ><AbsoluteDim value="0" /></Dim>
                    <Dim type="Width" >
                        <UnifiedDim scale="1" offset="-10" type="Width">
                            <OperatorDim op="Subtract">
                                <UnifiedDim scale="1" type="Height" />
                            </OperatorDim>
                        </UnifiedDim>
                    </Dim>
                    <Dim type="Height" ><UnifiedDim scale="1" type="Height" /></Dim>
                </Area>
                <ColourProperty name="CaptionColour" />
                <VertFormat type="CentreAligned" />
            </TextComponent>
        </ImagerySection>


        <ImagerySection name="glyph">
            <ImageryComponent>
                <Area>
                    <Dim type="LeftEdge">
                        <ImageDim imageset="Resummoned" image="TitlebarBorder_L" dimension="Width" />
                    </Dim>
                    <Dim type="TopEdge">
                        <UnifiedDim scale="0.5" type="Height">
                            <OperatorDim op="Subtract">
                                <ImageDim imageset="Resummoned" image="EmptyGlyph24" dimension="Height">
                                    <OperatorDim op="Divide">
                                        <AbsoluteDim value="2" />
                                    </OperatorDim>
                                </ImageDim>
                            </OperatorDim>
                        </UnifiedDim>
                    </Dim>
                    <Dim type="Width">
                        <ImageDim imageset="Resummoned" image="EmptyGlyph24" dimension="Width"/>
                    </Dim>
                    <Dim type="Height">
                        <ImageDim imageset="Resummoned" image="EmptyGlyph24" dimension="Height" />
                    </Dim>
                </Area>
                <ImageProperty name="GlyphImage" />
                <VertFormat type="Stretched" />
                <HorzFormat type="Stretched" />
            </ImageryComponent>
        </ImagerySection>

        
        <StateImagery name="Active">
            <Layer>
                <Section section="background_section">
                    <ColourProperty name="GS_MainThemeColour" />
                </Section>
                <Section section="background_highlight">
                    <ColourProperty name="GS_ShineColour" />
                </Section>
            </Layer>
            <Layer>
                <Section section="border_section" />
                <Section section="caption" >
                </Section>
                <Section section="glyph" />
            </Layer>
        </StateImagery>
        
        <StateImagery name="Inactive">
            <Layer>
                <Section section="background_section">
                    <ColourProperty name="GS_DesaturatedThemeColour" />
                </Section>
                <Section section="background_highlight">
                    <ColourProperty name="GS_ShineColour" />
                </Section>
            </Layer>
            <Layer>
                <Section section="border_section" />
                <Section section="caption" >
                </Section>
                <Section section="glyph" />
            </Layer>
        </StateImagery>
        
        <StateImagery name="Disabled">
            <Layer>
                <Section section="background_section">
                    <ColourProperty name="GS_MainThemeColour" />
                    <Colours topLeft="997F7F7F" topRight="997F7F7F" bottomLeft="997F7F7F" bottomRight="997F7F7F" />
                </Section>
                <Section section="background_highlight">
                    <ColourProperty name="GS_ShineColour" />
                </Section>
            </Layer>
            <Layer>
                <Section section="border_section" />
                <Section section="caption" >
                </Section>
                <Section section="glyph" />
            </Layer>
        </StateImagery>
    </WidgetLook>

