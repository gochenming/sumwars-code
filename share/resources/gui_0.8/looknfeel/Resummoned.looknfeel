                <Image component="TopRightCorner" imageset="Resummoned" image="MultiLineEditboxTopRight" />
                <Image component="BottomLeftCorner" imageset="Resummoned" image="MultiLineEditboxBottomLeft" />
                <Image component="BottomRightCorner" imageset="Resummoned" image="MultiLineEditboxBottomRight" />
                <Image component="LeftEdge" imageset="Resummoned" image="MultiLineEditboxLeft" />
                <Image component="RightEdge" imageset="Resummoned" image="MultiLineEditboxRight" />
                <Image component="TopEdge" imageset="Resummoned" image="MultiLineEditboxTop" />
                <Image component="BottomEdge" imageset="Resummoned" image="MultiLineEditboxBottom" />
                <Image component="Background" imageset="Resummoned" image="MultiLineEditboxBackdrop" />
            </FrameComponent>
        </ImagerySection>
        <ImagerySection name="Caret">
            <ImageryComponent>
                <Area>
                    <Dim type="LeftEdge" ><AbsoluteDim value="0" /></Dim>
                    <Dim type="TopEdge" ><AbsoluteDim value="0" /></Dim>
                    <Dim type="Width" ><ImageDim imageset="Resummoned" image="EditBoxCarat" dimension="Width" /></Dim>
                    <Dim type="BottomEdge" ><UnifiedDim scale="1.0" type="BottomEdge" /></Dim>
                </Area>
                <Image imageset="Resummoned" image="EditBoxCarat" />
                <VertFormat type="Stretched" />
                <HorzFormat type="Stretched" />
            </ImageryComponent>
        </ImagerySection>
        <StateImagery name="Enabled">
            <Layer>
                <Section section="main" />
            </Layer>
        </StateImagery>
        <StateImagery name="ReadOnly">
            <Layer>
                <Section section="main" />
            </Layer>
        </StateImagery>
        <StateImagery name="Disabled">
            <Layer>
                <Section section="main">
                    <Colours topLeft="FF7F7F7F" topRight="FF7F7F7F" bottomLeft="FF7F7F7F" bottomRight="FF7F7F7F" />
                </Section>
