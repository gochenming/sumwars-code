                <Image component="TopEdge" imageset="Resummoned" image="TabButtonBorder_T" />
                <Image component="Background" imageset="Resummoned" image="TabButtonBorder_M" />
                <Image component="BottomEdge" imageset="Resummoned" image="TabButtonBorder_B" />
                <VertFormat type="Stretched" />
                <HorzFormat type="Stretched" />
            </FrameComponent>
        </ImagerySection>

        <ImagerySection name="background_section">
