                <VertFormat type="Stretched" />
                <HorzFormat type="RightAligned" />
            </ImageryComponent>
        </ImagerySection>
        
        <ImagerySection name="ascend_icon">
            <ImageryComponent>
                <Area>
                    <Dim type="LeftEdge" ><AbsoluteDim value="8" /></Dim>
                    <Dim type="TopEdge" ><AbsoluteDim value="8" /></Dim>
                    <Dim type="Width" >
