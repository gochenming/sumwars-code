    <!--
    ***************************************************
        Resummoned/GroupBox
        (based on the Taharez Look)
    ***************************************************
    -->
    <WidgetLook name="Resummoned/GroupBox">
        <!-- TODO: next property is ignored ATM. Re-investigate appropriateness. -->
        <!--<PropertyDefinition name="BackgroundTextColours" initialValue="tl:00000000 tr:00000000 bl:00000000 br:00000000" redrawOnWrite="true" />-->
  	    <PropertyDefinition name="DisabledTextColour" initialValue="FF888888" redrawOnWrite="true" />
        <PropertyDefinition name="NormalTextColour" initialValue="FFFFFFFF" redrawOnWrite="true" />
        <PropertyDefinition name="TextBackground" initialValue="FF888888" redrawOnWrite="true" />
        <PropertyDefinition name="CaptionOffset" initialValue="10" redrawOnWrite="true" />

        <Child type="DefaultWindow" nameSuffix="__auto_contentpane__">
            <Area>
                <Dim type="LeftEdge">
                    <AbsoluteDim value="8">
                    </AbsoluteDim>
                </Dim>
                <Dim type="TopEdge">
                    <AbsoluteDim value="20" />
                </Dim>
                <Dim type="RightEdge">
                    <UnifiedDim scale="1" offset="0" type="RightEdge">
                        <OperatorDim op="Subtract">
                            <AbsoluteDim value="8" />
                        </OperatorDim>
                    </UnifiedDim>
                </Dim>
                <Dim type="BottomEdge">
                    <UnifiedDim scale="1" offset="0" type="BottomEdge">
                        <OperatorDim op="Subtract">
                            <AbsoluteDim value="8" />
                        </OperatorDim>
                    </UnifiedDim>
                </Dim>
            </Area>
        </Child>
        
        <ImagerySection name="label">
            <TextComponent>
                <Area>
                    <Dim type="LeftEdge">
                        <AbsoluteDim value="11">
                            <OperatorDim op="Add">
                                <PropertyDim name="CaptionOffset" />
                            </OperatorDim>
                        </AbsoluteDim>
                    </Dim>
                    <Dim type="TopEdge">
                        <AbsoluteDim value="3" />
                    </Dim>
                    <Dim type="RightEdge">
                        <UnifiedDim scale="1" offset="0" type="RightEdge" />
                    </Dim>
                    <Dim type="BottomEdge">
                        <UnifiedDim scale="1" offset="0" type="BottomEdge" />
                    </Dim>
                </Area>
                <VertFormat type="TopAligned" />
                <HorzFormat type="LeftAligned" />
            </TextComponent>
        </ImagerySection>
        <ImagerySection name="frame">
            <FrameComponent>
                <Area>
                    <Dim type="LeftEdge">
                        <AbsoluteDim value="0" />
                    </Dim>
                    <Dim type="TopEdge">
                        <AbsoluteDim value="11" />
                    </Dim>
                    <Dim type="RightEdge">
                        <UnifiedDim scale="1" offset="0" type="RightEdge" />
                    </Dim>
                    <Dim type="BottomEdge">
                        <UnifiedDim scale="1" offset="0" type="BottomEdge" />
                    </Dim>
                </Area>
                <Image component="TopLeftCorner" imageset="Resummoned" image="StaticTopLeft" />
                <Image component="TopRightCorner" imageset="Resummoned" image="StaticTopRight" />
                <Image component="BottomLeftCorner" imageset="Resummoned" image="StaticBottomLeft" />
                <Image component="BottomRightCorner" imageset="Resummoned" image="StaticBottomRight" />
                <Image component="LeftEdge" imageset="Resummoned" image="StaticLeft" />
                <Image component="TopEdge" imageset="Resummoned" image="StaticTop" />
                <Image component="RightEdge" imageset="Resummoned" image="StaticRight" />
                <Image component="BottomEdge" imageset="Resummoned" image="StaticBottom" />
                <VertFormat type="Stretched" />
                <HorzFormat type="Stretched" />
            </FrameComponent>
        </ImagerySection>
        <ImagerySection name="label_bg">
            <FrameComponent>
                <Area>
                    <Dim type="LeftEdge" >
                        <AbsoluteDim value="5">
                            <OperatorDim op="Add">
                                <PropertyDim name="CaptionOffset" />
                            </OperatorDim>
                        </AbsoluteDim>
                    </Dim>
                    <Dim type="TopEdge" >
                        <AbsoluteDim value="0" />
                    </Dim>
                    <Dim type="Width" >
                        <FontDim type="HorzExtent" padding="5">
                            <OperatorDim op="Add">
                                <AbsoluteDim value="8" />
                            </OperatorDim>
                        </FontDim>
                    </Dim>
                    <Dim type="Height" >
                        <FontDim type="LineSpacing">
                            <OperatorDim op="Add">
                                <AbsoluteDim value="6" />
                            </OperatorDim>
                        </FontDim>
                    </Dim>
                </Area>
                <!--<Image imageset="Resummoned" image="StaticBackdrop" />-->
                <Image component="TopLeftCorner" imageset="Resummoned" image="StaticTopLeft" />
                <Image component="LeftEdge" imageset="Resummoned" image="StaticLeft" />
                <Image component="BottomLeftCorner" imageset="Resummoned" image="StaticBottomLeft" />

                <Image component="TopEdge" imageset="Resummoned" image="StaticTop" />
                <Image component="BottomEdge" imageset="Resummoned" image="StaticBottom" />
                <Image component="Background" imageset="Resummoned" image="StaticBackdrop" />
                <Image component="TopRightCorner" imageset="Resummoned" image="StaticTopRight" />
                <Image component="BottomRightCorner" imageset="Resummoned" image="StaticBottomRight" />
                <Image component="RightEdge" imageset="Resummoned" image="StaticRight" />
                
                <!--<ColourRectProperty name="BackgroundTextColours" />-->
                <VertFormat type="Stretched" />
                <HorzFormat type="Stretched" />
            </FrameComponent>
        </ImagerySection>
        <StateImagery name="Enabled">
            <Layer>
                <Section section="frame" />
                <Section section="label_bg" />
                <Section section="label">
                    <ColourProperty name="NormalTextColour" />
                </Section>
            </Layer>
        </StateImagery>
        <StateImagery name="Disabled">
            <Layer>
                <Section section="frame" />
                <Section section="label_bg" />
                <Section section="label">
                    <ColourProperty name="DisabledTextColour" />
                </Section>
            </Layer>
        </StateImagery>
    </WidgetLook>



