                </Dim>
                <Dim type="Width">
                    <WidgetDim widget="__auto_TabPane__Buttons" dimension="Height">
                        <OperatorDim op="Multiply">
                            <PropertyDim name="TCScrollLRatio"/>
                        </OperatorDim>
                    </WidgetDim>
                </Dim>
