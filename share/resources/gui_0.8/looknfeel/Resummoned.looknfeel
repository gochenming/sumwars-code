                    <PropertyDim widget="__auto_TabPane__" name="EnableBottom">
                        <OperatorDim op="Multiply">
                            <PropertyDim name="TabHeight" type="Height" />
                        </OperatorDim>
                    </PropertyDim>
                </Dim>
                <Dim type="Height">
                    <UnifiedDim scale="1" type="Height">
                        <OperatorDim op="Subtract">
                            <PropertyDim name="TabHeight" type="Height" />
                        </OperatorDim>
                    </UnifiedDim>
                </Dim>
            </Area>
        </Child>
        <Child  type="Resummoned/TabButtonPane" nameSuffix="__auto_TabPane__Buttons">
            <Area>
                <Dim type="LeftEdge">
                    <PropertyDim name="TabHeight" type="Height" >
                        <OperatorDim op="Multiply">
                            <PropertyDim name="TCScrollLRatio"/>
                        </OperatorDim>
                    </PropertyDim>
                </Dim>
                <Dim type="RightEdge">
                    <UnifiedDim scale="1" type="Width" >
                        <OperatorDim op="Subtract">
                            <PropertyDim name="TabHeight" type="Height" >
                                <OperatorDim op="Multiply">
                                    <PropertyDim name="TCScrollLRatio"/>
                                </OperatorDim>
                            </PropertyDim>
                        </OperatorDim>
                    </UnifiedDim>
                </Dim>
                <Dim type="TopEdge">
                    <AbsoluteDim value="1">
                        <OperatorDim op="Add">
                            <PropertyDim widget="__auto_TabPane__Buttons" name="EnableTop">
                                <OperatorDim op="Multiply">
                                    <UnifiedDim scale="1" type="Height">
                                        <OperatorDim op="Subtract">
                                            <PropertyDim name="TabHeight" type="Height" />
                                        </OperatorDim>
                                    </UnifiedDim>
                                </OperatorDim>
                            </PropertyDim>
                        </OperatorDim>
                    </AbsoluteDim>
                </Dim>
                <Dim type="Height"><PropertyDim name="TabHeight" type="Height" /></Dim>
            </Area>
        </Child>
        <Child type="Resummoned/GlyphButton" nameSuffix="__auto_TabPane__ScrollLeft">
