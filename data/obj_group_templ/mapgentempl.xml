<ObjectGroupTemplate name = "trees3" >
        <Shape
                  type = "RECT"
                  extent_x = "4.0"
                  extent_y = "4.0"
                  angle ="0"
                  />

        <ObjectContent>
                <Object subtype="$tree"
                                center_x="2.5"
                                center_y="3.6"
                                angle = "0"
                                probability ="0.7"
                                prob_angle = "true"
                                />

                <Object subtype="$tree"
                                center_x="-1"
                                center_y="-2.3"
                                angle = "0"
                                probability ="0.7"
                                prob_angle = "true"
                                />

                <Object subtype="$tree"
                                center_x="-1.7"
                                center_y="3.3"
                                angle = "0"
                                probability ="0.7"
                                prob_angle = "true"
                                />

                <Object subtype="$tree"
                                center_x="0.7"
                                center_y="-1.7"
                                angle = "0"
                                probability ="0.7"
                                prob_angle = "true"
                                />
        </ObjectContent>

</ObjectGroupTemplate>

