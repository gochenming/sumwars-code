Hit
{
    material        flareAlpha
    particle_width  10
    particle_height 10
	cull_each       false
    quota           5000
    billboard_type  point
    
    // Area emitter
    emitter Point
{
        angle           360
        emission_rate   600
        time_to_live    0.3
        direction       0 1 0
        velocity_min    50
        velocity_max    50
        colour_range_start  1 0 0 
        colour_range_end    1 0 0 
        width           100
        height          100
        depth           30
}

    // Make em float upwards
    affector LinearForce
{
        force_vector      0 -200 0
        force_application add
}

}

