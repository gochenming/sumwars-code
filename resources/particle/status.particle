Blind
{
    material        flareAlpha
    particle_width  30
    particle_height 30
    cull_each       false
	cull_each       false
    quota          2000
    billboard_type  point
    
    // Area emitter
    emitter Box
{
        angle           360
        emission_rate   300
		position		0 -5 5
        time_to_live    0.1
        direction       0 1 0
        velocity    	50
        colour_range_start  0 0 0
		colour_range_end  0 0 0 0.2
		width           20
        height          20
        depth           20
}
}

Poison
{
    material        FlarePointSprite
    particle_width  30
    particle_height 30
    cull_each       false
	cull_each       false
    quota           1000
    billboard_type  point
    
    // Area emitter
    emitter Box
{
        angle           30
        emission_rate   100
        time_to_live    0.3
        position	0 50 0
        direction       0 1 0
        velocity    	0
        colour_range_start  0 1 0
        colour_range_end    0.4 1 0.3
        width           40
        height          40
        depth           120
}

    // Make em float upwards
    affector LinearForce
{
        force_vector      0 100 0
        force_application add
}

    // Fader
    affector ColourFader
{
        red -0.25
        green -0.25
        blue -0.25
}


}

